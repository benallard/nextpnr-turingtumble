library(turingtumble) {
    technology(custom); // at least, not cmos
    cell(RAMP_LEFT){
        pin(i_right, i_left) {direction: input;}
        pin(o_left) {
            direction: output;
            function: "i_right+i_left";
        }
    }
    cell(RAMP_RIGHT){
        pin(i_right, i_left) {direction: input;}
        pin(o_right) {
            direction: output;
            function: "i_right+i_left";
        }
    }
    cell (CROSS_OVER){
        pin(i_right, i_left) {direction: input;}
        pin(o_right) {
            direction: output;
            function: "i_left";
        }
        pin(o_left) {
            direction: output;
            function: "i_right";
        }
    }
    cell (INTERCEPTOR){
        pin(i_right, i_left) {direction: input;}
        ; // empty statement
    }
    cell (BIT){
        pin(i_right, i_left) {direction: input;}
        pin(i_both){direction: internal; function: "i_right + i_left";}
        pin(not_v){direction: internal; function: "IQN";}
        ff(IQ, IQN){
            clocked_on: "i_both";
            next_state: "not_v";
        }
        pin(o_right) {
            direction: output;
            function: "IQ*i_both";
        }
        pin(o_left) {
            direction: output;
            function: "IQN*i_both";
        }
    }
}